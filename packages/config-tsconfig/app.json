{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
      "allowJs": true,
      "target": "es5",
      "noImplicitAny": false,
      "incremental": true,
      "noImplicitThis": true,
      "noEmit": true,
      "downlevelIteration": true,
      "jsx": "preserve",
      "importHelpers": true,
      "removeComments": true,
      "resolveJsonModule": true
    }
}
  